      "cordova-plugin-facebook4": {
        "APP_ID": "2012482058985830",
        "APP_NAME": "Pear"
      },
      "cordova-plugin-x-socialsharing": {},
      "cordova-plugin-fcm": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
